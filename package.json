{
  "name": "kitten-care-app",
  "private": true,
  "version": "0.0.1",
  "type": "commonjs",
  "main": "electron/electron.js",
  "author": "Louis Lee",
  "description": "Sweet little kitty",
  "scripts": {
    "dev": "vite",
    "serve": "vite --host 0.0.0.0 --port 5173",
    "build": "vue-tsc --noEmit && vite build",
    "electron": "electron .",
    "electron:builder": "electron-builder",
    "build:e:pre": "vue-tsc --noEmit && cross-env ELECTRON=true vite build",
    "build:e": "npm run build:e:pre && npm run electron:builder"
  },
  "build": {
    "appId": "com.kittencare.app",
    "productName": "KittenCare",
    "copyright": "Copyright Â© 2019 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "dist_electron"
    }
  },
  "dependencies": {
    "pinia": "^2.0.32",
    "velocity-animate": "^1.5.2",
    "vue": "^3.2.47",
    "vue-router": "^4.2.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "electron": "^24.3.0",
    "electron-builder": "^23.6.0",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "pulltorefreshjs": "^0.1.22",
    "sass": "^1.60.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vue-tsc": "^1.4.2",
    "wait-on": "^7.0.1"
  }
}